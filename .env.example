# Example environment configuration for langrepl

# Log settings
LOG_LEVEL=INFO

# LLM API Keys
LLM__OPENAI_API_KEY=your_openai_api_key_here
LLM__ANTHROPIC_API_KEY=your_anthropic_api_key_here
LLM__GOOGLE_API_KEY=your_google_api_key_here
LLM__DEEPSEEK_API_KEY=your_deepseek_api_key_here
LLM__ZHIPUAI_API_KEY=your_zhipuai_api_key_here

# AWS Credentials (for Bedrock)
LLM__AWS_ACCESS_KEY_ID=your_aws_access_key_here
LLM__AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
LLM__AWS_SESSION_TOKEN=your_aws_session_token_here

# Local LLM URLs
LLM__OLLAMA_BASE_URL=http://localhost:11434
LLM__LMSTUDIO_BASE_URL=http://localhost:1234/v1

# Tracing and Observability

# LangSmith (https://smith.langchain.com)
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT="https://api.smith.langchain.com"
LANGCHAIN_API_KEY="your_langsmith_api_key"
LANGCHAIN_PROJECT="your_project_name"
